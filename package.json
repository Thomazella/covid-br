{
  "name": "corona",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fast-csv/parse": "^4.1.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.35",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "ts-node": "^8.8.2",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "frontend:start": "react-scripts start",
    "frontend:build": "react-scripts build",
    "frontend:test": "react-scripts test",
    "data:build": "ts-node --project data/tsconfig.json data/index.ts",
    "start": "npm run data:build && npm run frontend:start",
    "build": "npm run data:build && npm run frontend:build",
    "format": "prettier --write 'src/**/*.{ts,tsx,js}' 'data/**/*.{ts,tsx,js}'",
    "lint": "eslint --ignore-path .gitignore src data"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
